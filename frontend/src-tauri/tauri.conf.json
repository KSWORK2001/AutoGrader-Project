{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "AutoGrade",
  "version": "0.1.0",
  "identifier": "com.autograde.app",
  "build": {
    "frontendDist": "../",
    "devUrl": "http://127.0.0.1:5173"
  },
  "app": {
    "windows": [
      {
        "title": "AutoGrade",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src 'self' http://127.0.0.1:8000 http://localhost:8000 'unsafe-inline' 'unsafe-eval' data: blob:;"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}